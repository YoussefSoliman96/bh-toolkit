generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Provider {
  id             Int        @id @default(autoincrement())
  title          Title
  name           String
  gender         Gender
  evaluation     Int
  followUp       Int
  languages      String     @default("English")
  ageRange       Int
  workingHours   String
  suboxone       Acceptance @default(NO)
  adhd           Acceptance @default(NO)
  bipolar        Acceptance @default(NO)
  clozapine      Acceptance @default(NO)
  autism         Acceptance @default(NO)
  painManagement Acceptance @default(NO)
  trauma         Acceptance @default(NO)
  neurode        Acceptance @default(NO)
  minors         Acceptance @default(NO)
}

enum Acceptance {
  YES
  NO
}

enum Title {
  NP
  MD
  DNP
  DO
}

enum Gender {
  Male
  Female
}
